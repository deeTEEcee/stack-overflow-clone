<div class="page-header">
  <%= t('page.question.subheader') %>
</div>
<div id="container">
<% @questions.each do |question| %>
  <div class="answered"><%= t('page.question.answers_count', :count => question.answers.count) %></div>
  <div class="summary">
    <p> <%= link_to question.title, question_path(question, question.title.parameterize) %> </p>
    <p> When was this last asked/modified?</p>
  </div>
<% end %>
</div>
<br>
